// manifest.json
// Front Desk Extension - Manifest File

{
    "manifest_version": 3,
    "name": "Front Desk Queue Manager",
    "version": "1.0",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}
