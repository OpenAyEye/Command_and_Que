{
    "manifest_version": 3,
    "name": "Register Queue Manager",
    "version": "1.1",
    "permissions": ["storage"],
    "host_permissions": ["ws://localhost:8080/"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}
